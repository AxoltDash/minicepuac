; Division segura, falla antes de interpretar si no se tiene una prueba 
; que el denominador no es cero.
(let (safeDiv : (number -> ({ y : number | y != 0 } -> number))
      (lambda : number -> ({ y : number | y != 0 } -> number) (x)
        (lambda : { y : number | y != 0 } -> number (y)
          (/ x y)
        )
      )
    )
  ((safeDiv 10) 2)
)